server {
    listen {{.ServerPort}};

    location / {
        root /www;
        try_files $uri $uri/index.html /index.html =404;
    }

   location /_info {
        # add_header Content-Type application/json;
        ## override content-type ##
        types { } default_type "application/json";
        alias /info.json;
    }


{{range .Proxies}}
    location {{.Path}} {
       proxy_pass {{.Backend}};
    }
{{end}}
}
