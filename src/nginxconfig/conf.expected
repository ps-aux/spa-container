server {
    listen 80;

    location / {
        root /www;
        try_files $uri $uri/index.html /index.html =404;
    }

    location /conf {
       root /www;
       try_files /conf.json =404;
    }


    location /foo {
       proxy_pass http://blabla;
    }

    location /bar {
       proxy_pass https://bar:123;
    }

}
